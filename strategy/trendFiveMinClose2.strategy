{
    "waitEvent" : ["openLong", "openShort", "closePosition"],
    "openLong" : {
        "formula" : "('short' == getDirection(${gOneMinListData}, ${listIndex}, 2)) and (${position} == 0)",
        "price" : "getBeforeClose(${gOneMinListData}, ${listIndex}, 1)",
        "count" : "position1000()"
    },
    "openShort" : {
        "formula" : "('long' == getDirection(${gOneMinListData}, ${listIndex}, 2)) and (${position} == 0)",
        "price" : "getBeforeClose(${gOneMinListData}, ${listIndex}, 1)",
        "count" : "position1000()"
    },
    "closePosition" : {
        "formula" : "(${nowTimeStamp} >= ${openPositionTimeStamp} + 60 * 5) and (${position} != 0)",
        "price" : "getBeforeClose(${gOneMinListData}, ${listIndex}, 1)"
    }
}
